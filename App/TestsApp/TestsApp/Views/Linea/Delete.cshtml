@model TestsApp.Models.Linea

@{
    string linea = new TestsApp.Models.TestsAppBDEntities().NombreTabla.First(r => r.Tabla == "Linea").NombreMostrar;
    ViewBag.Title = "Delete";
}
<h3>¿Estás seguro que deseas eliminar este registro?</h3>
<fieldset>
    <legend>@linea</legend>

    <div class="display-label">
         @Html.DisplayNameFor(model => model.Nombre):
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Nombre)
    </div>
</fieldset>
@using (Html.BeginForm()) {
    <p>
        <input type="submit" value="Eliminar" class="btn btn-small red" /> |
        @Html.ActionLink("Regresar a la lista", "Index")
    </p>
}
