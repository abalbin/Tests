@model IEnumerable<TestsApp.Models.ExamenUsuario>
@{
    string aster = new TestsApp.Models.TestsAppBDEntities().NombreTabla.First(r => r.Tabla == "Aster").NombreMostrar;
}

@if (Model.Count() > 0)
{
    int idExamen = Model.ToList()[0].Examen.Id;
    @Html.ActionLink("Exportar a Excel", "ExportReportGraficaExamen", new { id = idExamen })    
}

<table class="table table-striped data-table table-examen">
    <thead>
        <tr>
            <th>
                @aster
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Puntaje)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Tiempo)
            </th>
            <th>Fecha
            </th>
        </tr>
    </thead>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @string.Format("{0} {1}", item.UserProfile.FirstName, item.UserProfile.LastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Puntaje)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tiempo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaInicio)
            </td>
        </tr>
    }

</table>


